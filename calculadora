var num1= 8375;
var num2= 6434;

//consolego.log (num1 = num2);

var soma
var resultado

function calcularSoma(num1,num2){
soma=num1 = num2;
return soma
}

resultado = calcularSoma(num1,num2);
console.log('A soma é:', resultado)

num1 = 4645
num2 = 4834

resultado = calcularSoma(num1,num2);
console.log ('A soma de', num1, 'e', num2, 'é', resultado) ;
